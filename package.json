{
    "name": "cs-dx-code-companion",
    "displayName": "CS SalesforceDX Code Companion",
    "description": "An extension that works alongside the Salesforce Extensions for Vscode with faster apex,vf and aura components save performance",
    "version": "0.0.2",
    "publisher": "MohithShrivastava",
    "engines": {
        "vscode": "^1.29.0"
    },
    "icon": "images/terminal.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/msrivastav13/DX-Code-Companion.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:sfdx-project.json"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "retrieve.dxSource",
                "title": "Retrieve DX Source From Package"
            },
            {
                "command": "deploy.source",
                "title": "Save To Salesforce"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "retrieve.dxSource",
                    "group": "DXCompanion"
                },
                {
                    "command": "deploy.source",
                    "group": "DXCompanion"
                }
            ],
            "editor/title": [
                {
                    "command": "retrieve.dxSource",
                    "group": "DXCompanion"
                },
                {
                    "command": "deploy.source",
                    "group": "DXCompanion"
                }
            ],
            "explorer/context": [
                {
                    "command": "retrieve.dxSource",
                    "group": "DXCompanion"
                },
                {
                    "command": "deploy.source",
                    "group": "DXCompanion"
                }
            ]
        },
        "keybindings": [
            {
                "command": "deploy.source",
                "key": "cmd+shift+c",
                "win": "ctrl+shift+c",
                "linux": "ctrl+shift+c",
                "when": "editorFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {}
}
